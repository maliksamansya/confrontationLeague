<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>List Tournament</title>
    <style>
        .container{
            width: 1000px;
            margin-left: auto;
            margin-right: auto;
            display: grid;
            grid-template-columns: auto auto auto;
        }

        img{
            width: 250px;
            height: 250px;
        }
    </style>
</head>
<body>
    <h1>List Tournament</h1>
    <div class="container">
        <% tournaments.forEach(el => { %>
            <div>
                <img src="<%= el.imageUrl %>" alt="">
                <p>CATEGORY: <%= el.category %></p>
                <p>Code: <%= el.code %></p>
                <br>
                <a href="/teams/<%= el.id %>/games"><button>JOIN</button></a>
                <a href="/teams/<%= el.id %>/teamtournament"><button>Check Team</button></a>
                <br>
                <br>
            </div>
        <% }) %>
    </div>
</body>
</html>